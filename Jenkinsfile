Jenkinsfile (Declarative Pipeline)
pipeline {
    agent any
    stages {
        stage('Cleanup') {
            steps {
                echo 'Cleanup...'
            }
        }
        stage('Checkout') {
            steps {
                echo 'Checkout..'
            }
        }
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}